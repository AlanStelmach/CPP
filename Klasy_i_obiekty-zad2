#include <iostream>

using namespace std;

class Point
{
private:
    float x, y;
public:
    void setX(float x)
    {
        this->x = x;
    }
    float getX()
    {
        return x;
    }
    void setY(float y)
    {
        this->y = y;
    }
    float getY()
    {
        return y;
    }
};

int main(int argc, char *argv[])
{
    Point p=Point();
    p.setX(5.5);
    p.setY(32.7);

    cout<<p.getX()<<endl;
    cout<<p.getY()<<endl;
    return 0;
}
