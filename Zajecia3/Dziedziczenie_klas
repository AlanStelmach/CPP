#include <iostream>

using namespace std;

class ssak
{
public:
    ssak() 
    {
    }
    void jedz()
    {
        cout<<"jem"<<endl;    
    }
};

class pies : public ssak
{
public:
    pies() 
    {
    }
    void szczekaj()
    {
      cout<<"szczekam"<<endl;  
    }
};

int main()
{
    pies.p=pies();
    p.szczekaj();
    p.jedz();
    
    return 0;
}

/* public:
        public -> public
        protected -> protected
        private -> nie dostępne  
   private:
        public -> private
        protected -> private
        private -> private
   protected:
        public -> protected
        protected -> protected
        private -> nie dostępne
*/
