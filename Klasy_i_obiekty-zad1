#include <iostream>

class trojkat
{
public:
    float a;
    float b;
    float c;
};

float fun( trojkat troj1)
{
    float x=troj1.a + troj1.b + troj1.c;
    return x;
}

int main()
{
    float obwod;
    trojkat troj1 = trojkat();
    troj1.a=6.0;
    troj1.b=5.0;
    troj1.c=11.6;

    obwod=fun(troj1);
    printf("%f", obwod);
    return 0;
}
