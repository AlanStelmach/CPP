#include <iostream>

using namespace std;

class Point
{
private:
    float x, y;
public:
    Point(float x, float y) //konstruktor(1) z 2 zmiennymi
    {
        this->x = x;
        this->y = y;
    }
    Point(float x) //konstruktor(2) z 1 zmiennÄ…
    {
        this->x = x;
        y= 0.0;
    }

    float getX()
    {
        return x;
    }
    float getY()
    {
        return y;
    }
    void move(Point p)
    {
        x += p.getX();
        y += p.getY();

    }
};

int main(int argc, char *argv[])
{
    Point p=Point(5.5, 8.0);
    Point w=Point(1.0, 2.0);
    //Point p=Point(5.5); //konstruktor(2)
    p.move(w);

    cout<<p.getX()<<endl;
    cout<<p.getY()<<endl;

    return 0;
}
